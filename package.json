  "main": "index.js",
  "dependencies": {
    "@google-cloud/storage": "^1.1.0",
    "aws-sdk": "^2.7.9",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-generator": "^6.24.0",
    "babel-preset-env": "^1.2.1",
    "babel-template": "^6.23.0",
    "babel-traverse": "^6.23.1",
    "babel-types": "^6.23.0",
    "babylon": "^6.16.1",
    "bluebird": "^3.5.0",
    "express": "^4.15.2",
    "glob": "^7.1.1",
    "js-beautify": "1.6.11",
    "js-yaml": "^3.8.2",
    "lodash": "4.17.4",
    "mkdir-recursive": "0.3.0",
    "readdir-recursive": "0.0.4",
    "request": "^2.81.0",
    "request-promise": "^4.1.1",
    "require-resolve": "0.0.2",
    "rmdir": "1.2.0",
    "serverless-google-cloudfunctions": "^1.0.0",
    "shelljs": "^0.7.7",
    "tape": "4.6.3",
    "uuid": "3.0.1",
    "yargonaut": "^1.1.2",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "tape src/**/test*.js",
    "index": "cd dist && node index.js",
    "deploy": "sh ./deploy.sh && sh ./local.sh",
    "local": "cd dist/files && node local.js",
    "live": "cd dist && node index.js --mode live",
    "lahc": "npm run build && npm run live && npm run local",
    "lint": "./node_modules/.bin/eslint **/*.js"
  },
